# UTHP Testing extlinux.conf file
LABEL TruckHackingOS Testing Image (Poky Scarthgap 5.0.2 reference distro)
        KERNEL ../zImage
        FDTDIR ../
        APPEND root=PARTUUID=${uuid} rootwait rw earlycon console=${console},${baudrate}
        FDTOVERLAYS     /lib/firmware/overlays/MCP251xFD-SPI.dtbo \
                        /lib/firmware/overlays/BB-UTHP-DCAN.dtbo
                        # /lib/firmware/overlays/<your-overlay>.dtbo